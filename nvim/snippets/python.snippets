snippet #!
  #!/usr/bin/env python3

snippet imp
  import ${0:module}

snippet from
  from ${1:package} import ${0:module}

snippet ignore "Ignore error / warning" w
  # type: ${1:ignore}[${2:import}]$0

snippet #ty "# type: ignore..." w
  # type: ${1:ignore}[${2:import}]$0

snippet myp "# mypy: ignore" b
  # mypy: ${1:disable-error-code}="${2:import}"$0

snippet ruf "# ruff: noqa" b
  # ruff: noqa: ${1:F811}$0
